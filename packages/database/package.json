{
  "name": "@onepiece-chat/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx,json,md}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx,json,md}\" --cache",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.9.0",
    "drizzle-orm": "^0.33.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  }
}
